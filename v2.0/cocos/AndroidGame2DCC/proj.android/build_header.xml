<project name="AndroidGame2DCC" default="BuildHeader">
    <target name="Stage">
    	<javah classpath="./bin/classes" destdir="../Classes/" class="com.plter.game2d.display.Stage"></javah>
    </target>
	
    <target name="DisplayObject">
    	<javah classpath="./bin/classes" destdir="../Classes/" class="com.plter.game2d.display.DisplayObject"></javah>
    </target>
	
    <target name="Image">
    	<javah classpath="./bin/classes" destdir="../Classes/" class="com.plter.game2d.display.Image"></javah>
    </target>

    <target name="DisplayObjectContainer">
    	<javah classpath="./bin/classes" destdir="../Classes/" class="com.plter.game2d.display.DisplayObjectContainer"></javah>
    </target>

    <target name="Sprite">
    	<javah classpath="./bin/classes" destdir="../Classes/" class="com.plter.game2d.display.Sprite"></javah>
    </target>
	
	<target name="Label">
    	<javah classpath="./bin/classes" destdir="../Classes/" class="com.plter.game2d.display.Label"></javah>
	</target>


    <target name="BuildHeader">
    	<antcall target="DisplayObject"></antcall>
        <antcall target="Stage"></antcall>
    	<antcall target="Image"></antcall>
    	<antcall target="DisplayObjectContainer"></antcall>
    	<antcall target="Sprite"></antcall>
    	<antcall target="Label"></antcall>
    </target>

</project>
